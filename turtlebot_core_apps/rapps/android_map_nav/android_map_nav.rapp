display:      Map Nav
description:  Drive a turtlebot around a pre-made map from an Android device.
platform:     linux.ros.turtlebot
launch:       turtlebot_core_apps/android_map_nav.launch
interface:    turtlebot_core_apps/android_map_nav.interface
icon:         turtlebot_core_apps/map_nav_bubble_icon.png
pairing_clients:
  - type: android
    manager:
      api-level: 9
      intent-action: com.github.rosjava.android_apps.map_nav.MainActivity
